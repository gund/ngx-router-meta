{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-router-meta": {
      "projectType": "library",
      "root": "projects/ngx-router-meta",
      "sourceRoot": "projects/ngx-router-meta/src",
      "prefix": "nrm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-router-meta/tsconfig.lib.json",
            "project": "projects/ngx-router-meta/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {}
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngx-router-meta/**/*.ts",
              "projects/ngx-router-meta/**/*.html"
            ]
          }
        }
      }
    },
    "demo-ssr": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/demo-ssr",
      "sourceRoot": "projects/demo-ssr/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-ssr/browser",
            "index": "projects/demo-ssr/src/index.html",
            "main": "projects/demo-ssr/src/main.ts",
            "polyfills": "projects/demo-ssr/src/polyfills.ts",
            "tsConfig": "projects/demo-ssr/tsconfig.app.json",
            "assets": [
              "projects/demo-ssr/src/favicon.ico",
              "projects/demo-ssr/src/assets"
            ],
            "styles": [
              "projects/demo-ssr/src/styles.css"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "ssr": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "namedChunks": false,
              "buildOptimizer": true
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo-ssr/src/environments/environment.ts",
                  "with": "projects/demo-ssr/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-ssr:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-ssr:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-ssr:build"
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/demo-ssr/server",
            "main": "projects/demo-ssr/server.ts",
            "tsConfig": "projects/demo-ssr/tsconfig.server.json",
            "optimization": {
              "scripts": false
            },
            "sourceMap": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo-ssr/src/environments/environment.ts",
                  "with": "projects/demo-ssr/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "demo-ssr:build",
            "serverTarget": "demo-ssr:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-ssr:build:production",
              "serverTarget": "demo-ssr:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "demo-ssr:build:production",
            "serverTarget": "demo-ssr:server:production",
            "routes": [
              "/",
              "/route1",
              "/route2",
              "/route3",
              "/lazy",
              "/lazy/child1",
              "/lazy/child2"
            ]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
